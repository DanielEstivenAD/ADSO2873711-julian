Proceso Ejercicio16
	//Calcular el área de un rectángulo de lados X e Y
	//DEFINICIÓN/DECLARACIÓN VARIABLES
	Definir area,X,YE Como Real;
	//ENTRADA DE DATOS
	Escribir "Asigne un valor al lado h=X ";
	leer X;
	Escribir "Asigne un valor al lado b=Y ";
	leer YE;
	//Proceso 
	area<-YE*X;
	//SALIDA
	Escribir "El área del rectángulo es: ",area;
FinProceso
	Escribir "Por favor agisnarle un número cualquiera a la letra (B): ";
	leer B;
	//Proceso 
	X<-A+B;
	//SALIDA
	Escribir "De acuerdo a los números asignados por el usaurio, el total de seta suma es :",X;
